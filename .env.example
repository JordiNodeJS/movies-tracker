# Environment Variables Example
# Copy this file to .env.local and fill in the actual values

# Neon Database Connection
DATABASE_URL="postgresql://user:password@host/database?sslmode=require&options=-csearch_path%3D%22movies-tracker%22"

# JWT Secret for authentication (minimum 32 characters)
JWT_SECRET="your-secret-key-here-minimum-32-characters"

# TMDB API Read Access Token (for movie data)
TMDB_READ_ACCESS_TOKEN="your_tmdb_token_here"

# Neon API Key (for Neon management operations)
NEON_API_KEY="napi_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"

# Vercel OIDC Token (auto-generated, not needed locally)
# VERCEL_OIDC_TOKEN="your-vercel-token-here"
